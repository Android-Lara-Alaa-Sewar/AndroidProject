<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context=".profileStudent">


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="-5dp"
        android:background="#90C39B"
        android:backgroundTint="#93C49C"
        app:cardElevation="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView5"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <Button
                android:id="@+id/profile_but"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_weight="1"
                android:backgroundTint="#92C39B"
                android:text="."
                android:textColor="#92C39B"
                app:icon="@drawable/baseline_person_24"
                app:iconSize="30dp" />

            <Button
                android:id="@+id/notification_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_weight="1"
                android:backgroundTint="#92C39B"
                android:text="."
                android:textColor="#92C39B"
                app:icon="@drawable/baseline_notifications_active_24"
                app:iconSize="30dp" />

            <Button
                android:id="@+id/chat_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:backgroundTint="#92C39B"
                android:text="."
                android:textColor="#92C39B"
                app:icon="@drawable/baseline_chat_24"
                app:iconSize="30dp" />

            <Button
                android:id="@+id/book_but"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_weight="1"
                android:backgroundTint="#92C39B"
                android:text="."
                android:textColor="#92C39B"
                app:icon="@drawable/baseline_library_books_24"
                app:iconSize="30dp" />

            <Button
                android:id="@+id/home_butM"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginEnd="10dp"
                android:layout_weight="1"
                android:backgroundTint="#92C39B"
                android:text="."
                android:textColor="#92C39B"
                app:icon="@drawable/baseline_home_24"
                app:iconSize="30dp" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView6"
        android:layout_width="396dp"
        android:layout_height="231dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="3dp"
        android:elevation="200dp"
        app:cardBackgroundColor="@color/Cream"
        app:cardCornerRadius="50dp"
        app:cardElevation="40dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="205dp">

            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="239dp"
                android:layout_height="205dp"
                android:layout_marginStart="30dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.369"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/studentprofilelogo"
                tools:layout_editor_absoluteY="0dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView7"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        app:cardBackgroundColor="@color/Cream"
        app:cardCornerRadius="50dp"
        app:cardElevation="90dp"
        app:layout_constraintBottom_toTopOf="@+id/cardView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView6"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <EditText
                android:id="@+id/EditTextName"
                style="@style/Widget.Material3.MaterialTimePicker.Display.HelperText"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:paddingStart="110dp"
                android:textColor="#71B18D"
                android:textSize="24sp"
                android:textStyle="bold" />

            <androidx.cardview.widget.CardView
                android:layout_width="200dp"
                android:layout_height="50dp"
                android:layout_marginStart="90dp"
                app:cardBackgroundColor="#71B18D"
                app:cardCornerRadius="50dp"
                app:cardElevation="30dp">

                <EditText
                    android:id="@+id/editTextId"
                    style="@style/Widget.AppCompat.TextView.SpinnerItem"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="20dp"
                    android:textColor="@color/Cream"
                    android:textSize="24sp"
                    android:textStyle="bold" />
            </androidx.cardview.widget.CardView>

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView5"
        android:layout_width="397dp"
        android:layout_height="419dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="25dp"
        app:cardBackgroundColor="#71B18D"
        app:cardCornerRadius="20dp"
        app:cardElevation="30dp"
        app:cardPreventCornerOverlap="false"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView6"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:layout_marginTop="55dp"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="10dp"
                    app:cardBackgroundColor="#71B18D"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView11"
                            style="@style/Widget.AppCompat.TextView.SpinnerItem"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="Semester Total"
                            android:textColor="@color/Cream"
                            android:textSize="20sp" />

                        <EditText
                            android:id="@+id/editTextNumber"
                            android:layout_width="63dp"
                            android:layout_height="68dp"
                            android:layout_marginStart="60dp"
                            android:ems="10"
                            android:inputType="number"
                            android:textSize="24sp" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="10dp"
                    app:cardBackgroundColor="#71B18D"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView20"
                            style="@style/Widget.AppCompat.TextView.SpinnerItem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:text="Completed Hours"
                            android:textColor="@color/Cream"
                            android:textSize="20sp" />

                        <EditText
                            android:id="@+id/editTextNumber3"
                            android:layout_width="53dp"
                            android:layout_height="68dp"
                            android:layout_marginStart="60dp"
                            android:ems="10"
                            android:inputType="number"
                            android:textSize="24sp" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="10dp"
                    app:cardBackgroundColor="#71B18D"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView22"
                            style="@style/Widget.AppCompat.TextView.SpinnerItem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="30dp"
                            android:text="Email"
                            android:textColor="@color/Cream"
                            android:textSize="20sp" />

                        <EditText
                            android:id="@+id/editTextTextEmailAddress"
                            android:layout_width="match_parent"
                            android:layout_height="67dp"
                            android:ems="10"
                            android:inputType="textEmailAddress"
                            android:textSize="20sp" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="200dp"
                android:layout_height="match_parent"
                android:layout_marginTop="55dp"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="10dp"
                    app:cardBackgroundColor="#71B18D"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView19"
                            style="@style/Widget.AppCompat.TextView.SpinnerItem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:text="GAP  Total"
                            android:textColor="@color/Cream"
                            android:textSize="20sp" />

                        <EditText
                            android:id="@+id/editTextNumber2"
                            android:layout_width="55dp"
                            android:layout_height="68dp"
                            android:layout_marginStart="60dp"
                            android:ems="10"
                            android:inputType="number"
                            android:textSize="24sp" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="10dp"
                    app:cardBackgroundColor="#71B18D"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView21"
                            style="@style/Widget.AppCompat.TextView.SpinnerItem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="Your favorit List"
                            android:textColor="@color/Cream"
                            android:textSize="20sp" />

                        <ImageButton
                            android:id="@+id/imageButton2"
                            style="@android:style/Widget.DeviceDefault.ImageButton"
                            android:layout_width="match_parent"
                            android:layout_height="57dp"
                            android:layout_margin="10dp"
                            android:backgroundTint="#71B18D"
                            android:importantForAccessibility="no"
                            app:srcCompat="@drawable/baseline_favorite_24" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="10dp"
                    app:cardBackgroundColor="#71B18D"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/textView24"
                            style="@style/Widget.AppCompat.TextView.SpinnerItem"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:text="Phone Namber"
                            android:textColor="@color/Cream"
                            android:textSize="20sp" />

                        <EditText
                            android:id="@+id/editTextNumber6"
                            android:layout_width="match_parent"
                            android:layout_height="68dp"
                            android:ems="10"
                            android:inputType="number"
                            android:textSize="24sp" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>
